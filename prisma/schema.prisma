generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model usuarios {
  id                             Int      @id @default(autoincrement())
  nome_completo                  String
  email                          String   @unique
  senha                          String
  tipo_usuario                   String
  permite_lancamento_retroativo Boolean  @default(false)
  criado_em                      DateTime @default(now())
}
